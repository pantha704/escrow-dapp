            "docs": [
              "Public key of the escrow maker"
            ],
            "type": "pubkey"
          },
          {
            "name": "mint_a",
            "docs": [
              "Mint of the token being deposited by the maker"
            ],
            "type": "pubkey"
          },
          {
            "name": "mint_b",
            "docs": [
              "Mint of the token expected to be received from the taker"
            ],
            "type": "pubkey"
          },
          {
            "name": "receive",
            "docs": [
              "Amount of mint_b tokens the maker expects to receive"
            ],
            "type": "u64"
          },
